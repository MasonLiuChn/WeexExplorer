    "weex-vue-router": "^0.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
